{
  "name": "frogteam",
  "displayName": "FrogTeam",
  "publisher": "SpiderInk",
  "description": "Your GenAI Team Workspace",
  "version": "0.0.9",
  "engines": {
    "vscode": "^1.91.0"
  },
  "categories": [
    "AI"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "frogteam.openProjectView",
        "title": "Open Project View"
      },
      {
        "command": "frogteam.openTeamLineupView",
        "title": "Open Team Lineup View"
      },
      {
        "command": "frogteam.openView",
        "title": "Open View"
      },
      {
        "command": "frogteam.refreshSetupView",
        "title": "Refresh Setup View"
      },
      {
        "command": "frogteam.openSetupView",
        "title": "Open Setup View"
      },
      {
        "command": "frogteam.refreshPromptView",
        "title": "Refresh Prompt View"
      },
      {
        "command": "frogteam.openPromptPanel",
        "title": "Open Prompt Panel"
      },
      {
        "command": "frogteam.addPrompt",
        "title": "New Prompt"
      },
      {
        "command": "frogteam.deletePrompt",
        "title": "Delete Prompt"
      },
      {
        "command": "frogteam.addSetup",
        "title": "New Member"
      },
      {
        "command": "frogteam.deleteSetup",
        "title": "Delete Member"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "frogteam",
          "title": "FrogTeam",
          "icon": "resources/icon.png"
        },
        {
          "id": "setupCollection",
          "title": "Setups",
          "icon": "resources/icon.png"
        },
        {
          "id": "promptCollection",
          "title": "Prompts",
          "icon": "resources/icon.png"
        }
      ]
    },
    "views": {
      "frogteam": [
        {
          "id": "projectView",
          "name": "Project"
        },
        {
          "id": "setupCollectionView",
          "name": "Team Configuration"
        },
        {
          "id": "promptCollectionView",
          "name": "Prompt Library"
        }
      ]
    },
    "menus": {
      "view/title": [
        {
          "command": "frogteam.addSetup",
          "when": "view == setupCollectionView",
          "group": "navigation"
        },
        {
          "command": "frogteam.addPrompt",
          "when": "view == promptCollectionView",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack --mode development",
    "watch": "webpack --mode development --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "20.x",
    "@types/vscode": "^1.90.0",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "esbuild": "^0.23.0",
    "eslint": "^8.57.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.4.5",
    "webpack": "^5.92.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@aws-crypto/sha256-js": "^5.2.0",
    "@aws-sdk/credential-provider-node": "^3.616.0",
    "@aws-sdk/types": "^3.609.0",
    "@langchain/community": "^0.2.22",
    "@langchain/core": "^0.2.19",
    "@smithy/eventstream-codec": "^2.0.5",
    "axios": "^1.7.2",
    "marked": "^13.0.2",
    "openai": "^4.52.2"
  },
  "files": [
    "out",
    "resources",
    "src",
    "package.json",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ampiazza/frogteam"
  },
  "license": "MIT",
  "icon": "resources/logo.png"
}
